{
  "modules": [
    {
      "name": "ConfigurationManager",
      "class": "mc.owls.valley.net.feathercore.core.common.ConfigurationManager",
      "mandatory": true,
      "dependencies": []
    },
    {
      "name": "MongoModule",
      "class": "mc.owls.valley.net.feathercore.modules.data.mongodb.MongoModule",
      "mandatory": true,
      "dependencies": ["ConfigurationManager"]
    },
    {
      "name": "PlayersDataModule",
      "class": "mc.owls.valley.net.feathercore.modules.data.players.PlayersDataModule",
      "mandatory": true,
      "dependencies": ["ConfigurationManager", "MongoModule"]
    },
    {
      "name": "FeatherEconomyProviderModule",
      "class": "mc.owls.valley.net.feathercore.modules.economy.provider.FeatherEconomyProviderModule",
      "mandatory": true,
      "dependencies": ["PlayersDataModule"]
    },
    {
      "name": "VaultModule",
      "class": "mc.owls.valley.net.feathercore.modules.economy.vault.VaultModule",
      "mandatory": true,
      "dependencies": ["FeatherEconomyProviderModule"]
    },
    {
      "name": "CommandsRegistrator",
      "class": "mc.owls.valley.net.feathercore.core.common.CommandsRegistrator",
      "mandatory": true,
      "dependencies": [
        "ConfigurationManager",
        "MongoModule",
        "PlayersDataModule",
        "FeatherEconomyProviderModule",
        "VaultModule"
      ]
    }
  ]
}
